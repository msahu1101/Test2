	<xs:element name="AMA_HotelGroupSearchRS">
		<xs:annotation>
			<xs:documentation>Search response based on the request criterion</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="Success">
					<xs:annotation>
