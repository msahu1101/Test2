												<xs:annotation>
													<xs:documentation>To be used with Search RS only. The Inventory week Start date of the Stay Over End Date as entered in the Search Criteria</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="EndDate" type="xs:date" use="required">
												<xs:annotation>
													<xs:documentation>To be used with Search RS only. The Inventory week End date of the Stay Over End Date as entered in the Search Criteria</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="Groups" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
