																			<xs:documentation>Internal Group ID create by the system and used to identify the group within AHP. My not not be accessible for other systems/channels. </xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																	<xs:attribute name="VersionNumber" type="VersionTimeStampType" use="required">
																		<xs:annotation>
																			<xs:documentation>Version number to prevent concurrent update.</xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																</xs:complexType>
															</xs:element>
															<xs:element name="GroupPeriod">
