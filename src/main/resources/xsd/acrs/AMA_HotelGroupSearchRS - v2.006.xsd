																			<xs:documentation>The Group Code is 3 characters long and used along with Property code and ("Initial Start Date" or "External ID") to identify group.</xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:string"/>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="GroupInternalID" type="RatePlanCodeType" use="required">
