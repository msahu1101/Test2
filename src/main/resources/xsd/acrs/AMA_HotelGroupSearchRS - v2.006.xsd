														<xs:documentation>This is a Group item. </xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Scope">
																<xs:complexType>
																	<xs:attributeGroup ref="GroupScopeType"/>
																	<xs:attribute name="GroupCode" use="required">
